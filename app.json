{
  "expo": {
    "name": "Toastmost",
    "slug": "toastmost",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/ToastmostMobileLogo.png",
    "scheme": "toastmost",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.carrcommunications.toastmost"
    },
    "android": {
      "package": "com.toastmost.mobileagenda",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ToastmostMobileLogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cc09ad29-3bdf-4bbc-860f-77deae4e0332"
      }
    },
    "owner": "davidfcarr"
  }
}
